function goGetToken() {
	if (!$('#fileInput').val().trim().length > 0
			&& !$('#imageUrl').val().trim().length > 0) {
		alert("Vui lòng chọn file hoặc nhập đường dẫn");
		return false;
	}
	$.ajax({
		type : "POST",
		url : "../account/toGetToken",
		dataType : "json",
		beforeSend : function() {
			$("#errorMessage").removeClass("red_text");
			$("#errorMessage").html(
					"<img src='../images/loading.gif' />  Đang xử lý ... ");
			$("#errorMessage").show("fast");
		},
		success : function(response) {
			if (response != "") {
				// alert("token " + response.token);
				updateAvatar(response.token);
			} else {
				return false;
			}
		},
		error : function(response) {
			return false;
		},
		complete : function() {

		}
	});
}
function updateAvatar(token) {
	var formData = new FormData($("#myForm")[0]);
	$("#errorMessage").addClass("red_text");
	$
			.ajax({
				url : 'http://172.16.18.198:8446/service/avatar/set?token='
						+ token,
				type : 'POST',
				contentType : false,
				dataType : "json",
				data : formData,
				async : false,
				success : function(data) {
					if (data.result == '0') {
						$("#errorMessage")
								.html(
										"<img src='../images/success.png' />  Thay đổi thành công. ");
						$('.avatarImage').attr(
								'src',
								$('.avatarImage').attr('src') + '?'
										+ Math.random());
					} else if (data.result == '9') {
						$("#errorMessage")
								.html(
										"<img src='../images/lose.png' />  Không tìm thấy ảnh. ");
					} else {
						$("#errorMessage")
								.html(
										"<img src='../images/lose.png' />  Thay đổi ảnh đại diện thất bại. ");
					}
					return false;
				},
				error : function(data) {
					$("#errorMessage")
							.html(
									"<img src='../images/lose.png' />  Thay đổi ảnh đại diện thất bại. ");
					return false;
				},
				cache : false,
				processData : false
			});
}

