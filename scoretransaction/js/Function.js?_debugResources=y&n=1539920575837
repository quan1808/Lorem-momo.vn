/* Hàm dùng ajax lấy html từ 1 URL vào 1 div */
function getHTMLContentInDiv(url , selector){
	//url là link cần lấy html
	// selector: '#content' or '.content '
	$.ajax({
		url : url,
		dataType : 'html',
		beforeSend : function() {
		},
		success : function(data) {
			$(selector).html(data);
		},
		error : function(request, status, error) {
		},
		complete : function() {
		},
		
	});
	
}

//function convert string format (12,000,000) to interger (currency)
function convertStringCurrency2Int(value){
	if(value == null || value == ""){
		return 0;
	}else{
		return parseInt(value.replace(/\,/g,''));
	}
	
}
// check validate phone 
function validatePhoneLogin(obj){
	
	if (!isPhoneNumber(obj.value)){ 
		//obj.value= ''; 
		obj.focus(); 
		$("#grailsmessage").hide();
		$('#errorMessage').html('Số điện thoại không hợp lệ');
	}
}
// ham lay gia tri cua 1 bien tren URL theo name
function GetURLParameter(sParam)
{
    var sPageURL = window.location.search.substring(1);
    var sURLVariables = sPageURL.split('&');
    for (var i = 0; i < sURLVariables.length; i++) 
    {
        var sParameterName = sURLVariables[i].split('=');
        if (sParameterName[0] == sParam) 
        {
            return sParameterName[1];
        }
    }
}
function showWaitting(){
	$('.waitting').show();
	$('.loadingimage').css('top', ($(window).height()/2)-50+"px");
	$('.loadingimage').css('left', ($(window).width()/2)-50 +"px");
}

function hideWaitting(){
	
	$('.waitting').hide();
}