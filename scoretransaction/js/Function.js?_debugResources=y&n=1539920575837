}

function updateStatusInformation(id, page, stt) {

	var ul = document.getElementsByName("ulforinfo");
	var p1 = document.getElementsByName("p1fordate");
	var f = stt - 1;

	document.getElementById(id).classList.add('normalInformation');
	document.getElementById(stt).classList.add('normalInformation');
	ul[f].className = ul[f].className.replace(/(?:^|\s)nav(?!\S)/g, '');
	ul[f].classList.add('nav1');
	if (window.XMLHttpRequest) {
		xmlhttp = new XMLHttpRequest();
	} else {
		xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
	}
	xmlhttp.onreadystatechange = function() {
		if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
			document.getElementById("notification").innerHTML = xmlhttp.responseText;

		}
	}
	xmlhttp.open("GET", "../notification/updateStatusOfInformation?idinfo="
			+ id + "&page=" + page, true);
	xmlhttp.send();

}

