function heightmenu() {
	setInterval(function() {
		var mainHight = document.getElementById('main-content').clientHeight;
		var leftMenuHieght = document.getElementById('pm_m_left').clientHeight;
		if (mainHight > leftMenuHieght) {
			$("#menu").css("height", mainHight);
		} else if (leftMenuHieght > mainHight) {
			var redundantHight = leftMenuHieght - mainHight;
			$("#menu").css("height", leftMenuHieght - redundantHight);
		}
		;
	}, 100);
}

$(function() {

	var menu_ul = $('.menu1 > li > ul'), menu_a = $('.menu1 > li > a');
	menu_ul.hide();
	menu_a.click(function(e) {

		e.preventDefault();
		if (!$(this).hasClass('active')) {
			menu_a.removeClass('active');
			menu_ul.filter(':visible').slideUp('normal');
			$(this).addClass('active').next().stop(true, true).slideDown(
					'normal');
		} else {
			$(this).removeClass('active');
			$(this).next().stop(true, true).slideUp('normal');

		}
		if ($("#ql").hasClass('active')) {
			reImageActive(document.getElementById("dv"));
			setImageActive2(document.getElementById("ql"));
		} else if ($("#dv").hasClass('active')) {
			reImageActive(document.getElementById("ql"));
			setImageActive2(document.getElementById("dv"));
		}
	});// reheightmenu();

});

function setheight() {
	var menu_ul = $('.menu2 > li > ul'), menu_a = $('.menu2 > li > a');
	menu_a.click(setTimeout(function(e) {
		if (e) {
			e.preventDefault();
			var heightoful = $("#ulsubmenu").height();

		}
	}), 1000);
}

function setclassActiveMenuteller(id) {
	if (id == "1") {
		document.getElementById("2").className = document.getElementById("2").className
				.replace(/(?:^|\s)active(?!\S)/g, '')
		document.getElementById("3").className = document.getElementById("3").className
				.replace(/(?:^|\s)active(?!\S)/g, '')
		document.getElementById("1").classList.add('active');
	}
	if (id == "2") {
		document.getElementById("1").className = document.getElementById("1").className
				.replace(/(?:^|\s)active(?!\S)/g, '')
		document.getElementById("3").className = document.getElementById("3").className
				.replace(/(?:^|\s)active(?!\S)/g, '')
		document.getElementById("2").classList.add('active');
	}
	if (id == "3") {
		document.getElementById("1").className = document.getElementById("1").className
				.replace(/(?:^|\s)active(?!\S)/g, '')
		document.getElementById("2").className = document.getElementById("2").className
				.replace(/(?:^|\s)active(?!\S)/g, '')
		document.getElementById("3").classList.add('active');
	}
}

$(document)
		.ready(
				function() {

					var title = $(document).find("title").text();
					if (title == "Chuyển tiền | Ví điện tử MoMo") {
						var ct = document.getElementById("ct");
						ct.classList.add('active');
						setImageActive(ct);
						// setTimeout(autoAddClass(), 1000);
					}
					if (title == "Nạp tiền | Ví điện tử MoMo") {
						var nt = document.getElementById("nt");
						nt.classList.add('active');
						setImageActive(nt);
						// setTimeout(autoAddClass(), 1000);
					}

					if (title == "Rút tiền | Ví điện tử MoMo") {
						var rt = document.getElementById("rt");
						rt.classList.add('active');
						setImageActive(rt);
						// setTimeout(autoAddClass, 1000);
					}
					if (title == "Thông tin tài khoản | Ví điện tử MoMo") {
						var ql = document.getElementById("ql");
						ql.classList.add('active');
						setImageActive2(ql);
						document.getElementById("tttk").classList
								.add('subMenuActive');
						document.getElementById("tttkLink").classList
								.add('subMenuLinkActive');
						$("#usermnu").slideDown();

						// ql.next().stop(true, true).slideDown('normal');
					}
					if (title == "Quản lý thẻ | Ví điện tử MoMo") {
						var ql = document.getElementById("ql");
						ql.classList.add('active');
						setImageActive2(ql);
						document.getElementById("qlt").classList
								.add('subMenuActive');
						document.getElementById("qltLink").classList
								.add('subMenuLinkActive');
						$("#usermnu").slideDown();

						// ql.next().stop(true, true).slideDown('normal');
					}
					if (title == "Lịch sử giao dịch | Ví điện tử MoMo") {
						var ql = document.getElementById("ql");
						ql.classList.add('active');
						setImageActive2(ql);
						document.getElementById("lsgd").classList
								.add('subMenuActive');
						document.getElementById("lsgdLink").classList
								.add('subMenuLinkActive');
						$("#usermnu").slideDown();
					}
					if (title == "Nạp điện thoại | Ví điện tử MoMo") {
						var dv = document.getElementById("dv");
						dv.classList.add('active');
						setImageActive2(dv);
						document.getElementById("ndt").classList
								.add('subMenuActive');
						document.getElementById("ndtLink").classList
								.add('subMenuLinkActive');
						$("#dvti").slideDown();
					}
					if (title == "Nạp game | Ví điện tử MoMo") {
						var dv = document.getElementById("dv");
						dv.classList.add('active');
						setImageActive2(dv);
						document.getElementById("ng").classList
								.add('subMenuActive');
						document.getElementById("ngLink").classList
								.add('subMenuLinkActive');
						$("#dvti").slideDown();
					}
					if (title == "Thanh toán hóa đơn | Ví điện tử MoMo") {
						var dv = document.getElementById("dv");
						dv.classList.add('active');
						setImageActive2(dv);
						document.getElementById("tthd").classList
								.add('subMenuActive');
						document.getElementById("tthdLink").classList
								.add('subMenuLinkActive');
						$("#dvti").slideDown();
					}
					if (title == "Thanh toán vé máy bay | Ví điện tử MoMo") {
						var dv = document.getElementById("dv");
						dv.classList.add('active');
						setImageActive2(dv);
						document.getElementById("ttvmb").classList
								.add('subMenuActive');
						document.getElementById("ttvmbLink").classList
								.add('subMenuLinkActive');
						$("#dvti").slideDown();
					}

					if (title == "Điểm giao dịch | Ví điện tử MoMo") {
						var ct = document.getElementById("dgd");
						ct.classList.add('active');
						setImageActive(ct);
						// setTimeout(autoAddClass(), 1000);
					}
					if (title == "Trợ giúp - Hướng dẫn | Ví điện tử MoMo") {
						var nt = document.getElementById("hd");
						nt.classList.add('active');
						setImageActive(nt);
						// setTimeout(autoAddClass(), 1000);
					}
				});
function setImageActive(e) {

	$(e).find(".firstImage").css("display", "none");
	$(e).find(".secondImage").css("display", "inline");
}
function setImageActive2(e) {

	$(e).find(".firstImage").css("display", "none");
	$(e).find(".secondImage").css("display", "inline");
	$(e).find(".thirdImage").css("display", "inline");
	$(e).find(".fourthImage").css("display", "none");

}
function reImageActive(e) {
	$(".nav").find(".firstImage").css("display", "inline");
	$(".nav").find(".secondImage").css("display", "none");
	$(e).find(".thirdImage").css("display", "none");
	$(e).find(".fourthImage").css("display", "inline");

}

function isPhoneNumber(checkstring) {	
	var regexDigit_01 =  "^01[0-9]{9}$";// "(0\\d{9,10})$";
	var regexDigit_09 = "^09[0-9]{8}$";
	var isValid = checkstring.match(regexDigit_01);
	
	if (isValid)
		return true;	
	else 
		{
			isValid = checkstring.match(regexDigit_09);
			if (isValid)				
				return true;						
		}
	return false;
	/*if (isValid == null || checkstring.substring(0, 2) == "00" 
			&& checkstring.length > 10 || checkstring.split(" ").length > 1)
		return false;
	else
		return true;*/
}

function getDistrictData(filename) {
	var valueSelected;
	var city = document.getElementById("city");
	valueSelected = city.value;
	var searchid = document.getElementById("searchid").value;
	var xmlhttp;
	if (window.XMLHttpRequest) {
		xmlhttp = new XMLHttpRequest();
	} else {
		xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
	}
	xmlhttp.onreadystatechange = function() {
		if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
			document.getElementById("distric").innerHTML = xmlhttp.responseText;
		}
	}
	xmlhttp.open("GET",
			"../scoretransaction/getDistrictdata?cid=" + valueSelected
					+ "&searchid=" + searchid + "&filename=" + filename, true);
	xmlhttp.send();
}

function centerMap(lat, longitude, id) {

	document.getElementById("map").contentWindow.moveToLocation(lat, longitude,
			id);

	var divstrore = document.getElementsByClassName("divstore");
	var len = divstrore.length;
	for (var i = 0; i < len; i++) {
		divstrore[i].className = divstrore[i].className.replace(
				/(?:^|\s)divstoreactive(?!\S)/g, '');
	}
	;
	document.getElementById(id).classList.add('divstoreactive');
}

function closeInfowindow() {

	document.getElementById("map").contentWindow.closeAllInfoWindows();
}

function validateEmail(email) {
	var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	return re.test(email);
}
function previouspage() {
	var url = window.location.href;
	var linkprevious = document.getElementById("previous");
	var page = document.getElementById("page").value;

	if (url.length >= 94) {
		if (page - 1 == 0) {

			linkprevious.href = "#"
		} else {
			var a = url.replace("pagecount=" + page, "pagecount=" + (page - 1));
			linkprevious.href = a;

		}

	} else {
		linkprevious.href = url + "&pagecount=1"
	}
	;

}
function nextpage() {
	var url = window.location.href;
	var linknext = document.getElementById("next");
	var page = document.getElementById("page").value;
	var pagecount = document.getElementById("pagecount").value;
	if (url.length >= 94) {
		if (page + 1 > pagecount) {

			linknext.href = "#"
		} else {

			var a = url.replace("pagecount=" + page, "pagecount="
					+ (page - 1 + 2));
			linknext.href = a;

		}

	} else {

		linknext.href = url + "&pagecount=2";

	}
	;

}
function Submit(formId, actionName) {
	var targetForm = document.getElementById(formId);
	var action = targetForm.action;
	targetForm.action = actionName;
	targetForm.submit();
	targetForm.action = action;
}

function doConfirm(buttonName, formId, actionName) {

	if (window.confirm("Bạn có muốn thực hiện việc " + buttonName)) {
		Submit(formId, actionName);
	}

	return;

}

