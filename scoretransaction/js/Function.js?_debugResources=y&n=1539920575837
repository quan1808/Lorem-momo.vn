function setCardInfo(ownerName, accountID, openDate) {
	var month = openDate.substring(0, 2);
	$("#txtBankCardOwner").val(ownerName);
	$("#txtBankCardCardId").val(accountID);
	if (month > 9) {
		$("#cbBankCardMonth option[value='" + openDate.substring(0, 2) + "']")
				.attr('selected', 'selected');
	} else {
		$("#cbBankCardMonth option[value='" + openDate.substring(1, 2) + "']")
				.attr('selected', 'selected');
	}

	$("#cbBankCardYear option[value='" + openDate.substring(3) + "']").attr(
			'selected', 'selected');
	$("#bankCardDiv").fadeIn(0001);
}
function checkOpenDate(month, year) {
	var date = new Date();
	var nYear = date.getFullYear();
	var nMonth = date.getMonth() + 1;
	if (year == nYear && month > nMonth) {
		return false;
	} else {
		return true;
	}
}
