$(document)
		.ready(
				function() {
					
					$("#moneyField").hide();
					//ngan khong cho cac ky tu dac biet vao input				
					$('input[type=text]').keyup(function(){
						text = replaceCharacter( $(this).val() );
						$(this).val( text );
					});
					$('input[type=text]').blur(function(){
						text = replaceCharacter( $(this).val() );
						$(this).val( text );
					});	
					$('input[type=text]').change(function(){
						
						text = replaceCharacter( $(this).val() );
						$(this).val( text );
					});		
					$('input[type=text]').on('paste', function(e){ 
						text = replaceCharacter( $(this).val() );
						$(this).val( text );
					});
					$('input[type=text]').focusout(function(){ 
						text = replaceCharacter( $(this).val() );
						$(this).val( text );
					});
					
					//end
					
					
					$('.hdcontent').hide();

					$("#fromDatepicker").datepicker(
							{
								dateFormat : 'dd/mm/yy',
								maxDate : '0',
								onSelect : function(selected) {
									$("#toDatepicker").datepicker("option",
											"minDate", selected)
								}
							}).attr('readonly', 'readonly');
					$("#toDatepicker").datepicker(
							{
								dateFormat : 'dd/mm/yy',
								maxDate : '0',
								onSelect : function(selected) {
									$("#fromDatepicker").datepicker("option",
											"maxDate", selected)
								}
							}).attr('readonly', 'readonly');
					$("#xacNhanNapTienTanNoi").hide();
					$("#confirm_tranfer").hide();
					$(".alert").addClass("in").fadeOut(4500);
					/* swap open/close side menu icons */
					$('[data-toggle=collapse]')
							.click(
									function() {// toggle icon
										$(this).find("i")
										heightmenu
												.toggleClass("glyphicon-chevron-right glyphicon-chevron-down");
									});
					$(".phone").text(
							function(i, text) {
								text = text.replace(/(\d{4,5})(\d{3})(\d{3})/,
										"$1 $2 $3");
								return text;
							});

					$("div#backgroundPopup").click(function() {
						disablePopup(); // function close pop up
					});
					$(".amount").text(function(i, text) {
						text = text.replace(/\B(?=(\d{3})+(?!\d))/g, ".");
						return text;
					});
					$(".bankID").text(
							function(i, text) {
								text = text.replace(/(\d{4})(\d{3,6})(\d{3})/,
										"$1******$3");
								return text;
							});

					if (typeof topupHide == 'function') {
						topupHide();
					}

					if (typeof topupNapTien == 'function') {
						topupNapTien();
					}

					if (typeof topupChuyenTien == 'function') {
						topupChuyenTien();
					}

					if (typeof topupRutTien == 'function') {
						topupRutTien();
					}

					if (typeof topupNapTienTanNoi == 'function') {
						topupNapTienTanNoi();
					}

					if (typeof topupChuyenTienNgay == 'function') {
						topupChuyenTienNgay();
					}

					$('input.phone')
							.keyup(
									function(event) {

										// skip for arrow keys
										if (event.which >= 37
												&& event.which <= 40)
											return;

										// format number
										$(this)
												.val(
														function(index, value) {
															return value
																	.replace(
																			/\D/g,
																			'')
																	.replace(
																			/(\d{4,5})(\d{3})(\d{3})/,
																			"$1 $2 $3");
														});
									});
					//	
					$('input.money')
							.keyup(
									function(event) {

										// skip for arrow keys
										if (event.which >= 37
												&& event.which <= 40)
											return;

										// format number
										$(this)
												.val(
														function(index, value) {
															return value
																	.replace(
																			/\D/g,
																			'')
																	.replace(
																			/\B(?=(\d{3})+(?!\d))/g,
																			",");
														});
									});
				});
function showPopUp(controller, action,type, param1, param2, param3, param4, param5) {
	//param1,param2,param3 la gia tri cac input tren form theo thu tu
	if (controller === "" || action === "" ) {
		alert('thieu thong tin');
		return false;
	}
	$.ajax({
		url : "../banknetLink/showBankResource",
		dataType : 'json',
		data : {
			mycontroller : controller,
			myaction : action,
			type:type,
			param1 : param1,
			param2 : param2,
			param3 : param3,
			param4: param4,
			param5: param5,
			
		},
		beforeSend : function() {
		},
		success : function(data) {
			if (data.message != null) {
				$("#errorMessageConfirm").html(data.message).addClass(
						"red_text");
				return false;
			}
			$("#toPopup").html("<div onclick='disablePopup()' class='close_popup' ></div>"+data.template);
			loading(); // loading
			setTimeout(function() { // then show popup, deley in .5 second
				loadPopup(); // function show popup
			}, 500); // .5 second
			return false;
		},
		error : function(request, status, error) {
			alert(error);
		},
		complete : function() {
		}
	});
}


function showPopUpComfirm() {
	$.ajax({
		url : "../agent/popup",
		
		success : function(data) {
			
			//$("#toPopup").html("<div onclick='disablePopup()' class='close_popup' ></div>"+data.template);
			$("#toPopup").html("<div onclick='disablePopup()' class='close_popup' ></div>"+data.template);
			loading(); // loading
			setTimeout(function() { // then show popup, deley in .5 second
				loadPopup(); // function show popup
			}, 500); // .5 second
			return false;
			
		},
		error : function(request, status, error) {
			alert(error);
		},
		complete : function() {
		}
	});
			
}

/** ************ start: functions. ************* */
$('*').keyup(function(event) {
	if (event.which == 27) { // 27 is 'Ecs' in the keyboard
		disablePopup(); // function close pop up
	}
});
function loading() {
	$("div.loader").show();
}
function closeloading() {
	$("div.loader").fadeOut('normal');
}

function loadPopup() {
	closeloading(); // fadeout loading
	// fadein popup div
	$("#backgroundPopup").css("opacity", "0.7"); // css opacity, supports
													// IE7, IE8
	$("#backgroundPopup").fadeIn(0001);
	$("#toPopup").fadeIn(0500);
}

function disablePopup() {
	$("#toPopup").fadeOut("normal");
	$("#backgroundPopup").fadeOut("normal");
}
